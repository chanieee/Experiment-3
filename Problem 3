#include <iostream>

using namespace std;

int main()
{
char character[5];

    cout << "Type any five characters: " << endl;
    for (int counter = 0; counter < 5; counter ++)
    {
        cin >> character[counter];
        cout << endl;
    }

    cout << endl;
    cout << "Characters in reverse order: " << endl;
    for (int counter2 = 4 ; counter2 >= 0; counter2--)
    {
        cout << counter2 + 1 << ") " << character[counter2] << endl;
        cout << endl;
    }
        cout << endl;
      
return 0;
}
